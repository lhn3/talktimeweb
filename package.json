    "normalize.css": "^8.0.1",
    "particles.js": "^2.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.1.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
